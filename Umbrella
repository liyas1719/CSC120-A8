import java.util.ArrayList;
public class Umbrella implements Contract {


public interface Contract {
    public Number size;
    public int xlocationfeet;
    public int ylocationfeet;
    public boolean directionfacing;

    public void grab(String item) {
        xlocation = 1;
        ylocation = 1;
        System.out.println("Umbrella has been picked up");
    }

    public String drop(String item) {
        if (inventory.contains(item)) {
            inventory.remove(item)
            String returning = item + "dropped";
            return returning;
        } else if (holding.contains(item)) {
            holding.remove(item)
            String returning = item + "dropped";
            return returning;
        } else {
            System.out.println("Item unable to be dropped.");
            return "Drop unsuccessful.";
        }
    }

    public void examine(String item);
    public void use(String item);
    public boolean walk(String direction);
    public boolean fly(int x, int y);
    public Number shrink();
    public Number grow();
    public void rest();
    public void undo();

}
